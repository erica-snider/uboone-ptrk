# uBooNE evd for LArSoft v04##include "evdservices_microboone.fcl"#include "caldata_microboone.fcl"#include "calibration_microboone.fcl"process_name: EVDservices:{  TFileService:            { fileName: "reco2D_hist_evd.root" }#  message:                 @local::evd_message  message:                 @local::microboone_message_services_prod_debug#  Timing:                  {}#  SimpleMemoryCheck:       { ignoreTotal: 1 } # default is one  RandomNumberGenerator:   {} #ART native random number generator  ChannelStatusService:    @local::microboone_channelstatus_service  			   @table::microboone_disp}# Define the services#Look at the input filessource:{  module_type: RootInput  fileNames:  [ "data.root" ]  maxEvents:   -1       # Number of events to create}outputs:{}# Define and configure some modules to do work on each event.# First modules are defined; they are scheduled later.# Modules are grouped by type.physics:{ producers: { } filters: { } analyzers: {   evdisp:{module_type: EVD} } reco: [       ]  #list the modules for this path, order matters, filters reject all following items evd: [ evdisp ]  #end_path are things that do not modify art::Event, includes analyzers  #and output modules. all items here can be run simultaneously end_paths: [evd]  }### Here, we overwrite ALL module labels with the ones defined above.# Adjust the window/offset for the reduced total frame size (9600 -> 6400 ticks)services.DetectorProperties.NumberTimeSamples:                             6400services.DetectorProperties.ReadOutWindowSize:                             6400services.TimeService.InheritClockConfig:                                   falseservices.TimeService.TriggerOffsetTPC:                                     -0.400e3### Turn off the database...#services.IChannelStatusService.ChannelStatusProvider.UseDB:           false#services.IDetPedestalService.DetPedestalRetrievalAlg.UseDB:           false#services.IDetPedestalService.DetPedestalRetrievalAlg.DefaultCollMean: 473#services.IDetPedestalService.DetPedestalRetrievalAlg.DefaultIndMean:  2046### Event display very basic set upservices.RawDrawingOptions.TotalTicks:                    6400services.ColorDrawingOptions.RawDiv:                      [  200,  200]services.ColorDrawingOptions.RawQLow:                     [ -100, -100]services.ColorDrawingOptions.RawQHigh:                    [  300,  300]services.ColorDrawingOptions.RecoDiv:                     [  200,  200]services.ColorDrawingOptions.RecoQLow:                    [ -100, -100]services.ColorDrawingOptions.RecoQHigh:                   [  300,  300]services.RawDrawingOptions.RawDataLabel:                  "digitfilter"services.RawDrawingOptions.MinimumSignal:                 10services.RawDrawingOptions.DrawRawDataOrCalibWires:       2